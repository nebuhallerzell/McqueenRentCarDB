@model CarBrands
<form method="post">
<div class="border p-3 mt-5">

    <div class="row pb-3">
            <h1 class="text-warning">Add Brand</h1><hr />
    </div>
    
    <div class="mb-3">
        <label asp-for="car_brand">Brand Name</label>
        <input asp-for="car_brand" type="text" class="form-control" />
        <span asp-validation-for="car_brand" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-outline-warning">Add Brand</button>
        <a asp-controller="CarBrands" asp-action="Index" class="btn btn-outline-warning">Back</a>
</div>
</form>
